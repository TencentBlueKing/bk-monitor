{
  "id": "container",
  "type": "detail",
  "mode": "auto",
  "name": "k8s",
  "variables": [],
  "panels": [],
  "overview_panels": [],
  "order": [],
  "options": {
    "panel_tool": {
      "compare_select": true,
      "columns_toggle": true,
      "interval_select": true,
      "split_switcher": false,
      "method_select": true
    },
    "show_panel_count": false,
    "enable_index_list": true,
    "enable_group": true,
    "alert_filterable": true,
    "selector_panel": {
      "title": "Pods",
      "type": "table",
      "targets": [
        {
          "datasource": "pod_list",
          "dataType": "list",
          "api": "apm_container.listServicePods",
          "data": {
            "app_name": "$app_name",
            "service_name": "$service_name",
            "start_time": "${start_time}",
            "end_time": "${end_time}"
          },
          "fields": {
            "bcs_cluster_id": "bcs_cluster_id",
            "namespace": "namespace",
            "pod_name": "name"
          }
        }
      ],
      "options": {
        "selector_list": {
          "query_update_url": true,
          "status_filter": true,
          "field_sort": true,
          "default_sort_field": "-limit_cpu_usage_ratio"
        }
      }
    },
    "detail_panel": {
      "title": "Pod",
      "type": "info",
      "targets": [
        {
          "datasource": "info",
          "dataType": "info",
          "api": "scene_view.getKubernetesPod",
          "data": {
            "bcs_cluster_id": "$bcs_cluster_id",
            "namespace": "$namespace",
            "pod_name": "$pod_name"
          }
        }
      ]
    },
    "group_panel": {
      "targets": [
        {
          "datasource": "dimensions",
          "dataType": "list",
          "api": "scene_view.getSceneViewDimensions",
          "data": {
            "scene_id": "kubernetes",
            "type": "detail",
            "id": "pod"
          }
        }
      ]
    }
  }
}