    {
      "id": 1,
      "title": "Apdex",
      "type": "apdex-chart",
      "gridPos": {
        "x": 0,
        "y": 0,
        "w": 12,
        "h": 4
      },
      "targets": [
        {
          "datasource": "apm",
          "dataType": "apdex",
          "api": "apm_metric.apdexQuery",
          "data": {
            "app_name": "$app_name",
            "interval": "$interval"
          }
        }
      ],
      "descrition": "根据Apdex进行划分，划分范围为：<br>1.红：0~0.25<br>2.黄：0.25~0.75<br>3.绿：0.75以上"
    },
