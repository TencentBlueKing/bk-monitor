{
  "include": ["./**/*.ts", "./**/*.vue", "./**/*.tsx", "./**/*.ts", ".eslintrc.cjs",],
  "exclude": ["node_modules"],
  "compilerOptions": {
    "allowSyntheticDefaultImports": true,
    "experimentalDecorators": true,
    "importHelpers": true,
    "strict": true,
    "strictBindCallApply": true,
    "strictNullChecks": false,
    "strictFunctionTypes": true,
    "strictPropertyInitialization": false,
    "noImplicitAny": false,
    "noImplicitThis": true,
    "isolatedModules": true,
    "resolveJsonModule": true,
    "allowJs": true,
    "module": "esnext",
    "target": "esnext",
    "outDir": "dist",
    "moduleResolution": "node",
    "rootDir": "../../../",
    "sourceMap": true,
    "pretty": true,
    "jsx": "preserve",
    "lib": ["es2018", "dom"],
    "paths": {
      "@/*": [
        "./*"
      ],
      "@router/*": [
        "./router/*"
      ],
      "@store/*": [
        "./store/*"
      ],
      "@page/*": [
        "./page/*"
      ],
      "@static/*": [
        "./static/*"
      ]
    }
  }
}
