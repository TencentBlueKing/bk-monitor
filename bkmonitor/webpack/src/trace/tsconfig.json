{
  "include": ["./**/*.ts", "./**/*.vue", "./**/*.tsx", "./**/*.ts"],
  "exclude": ["node_modules"],
  "extends": "../../tsconfig.base.json",
  "compilerOptions": {
    "outDir": "./dist",
    "strict": true,
    "strictBindCallApply": true,
    "strictNullChecks": false,
    "strictFunctionTypes": true,
    "strictPropertyInitialization": false,
    "noImplicitAny": false,
    "noImplicitThis": true,
    "baseUrl": ".",
    "paths": {
      "@/*": ["./*"],
      "@router/*": ["./router/*"],
      "@store/*": ["./store/*"],
      "@page/*": ["./pages/*"],
      "@static/*": ["./static/*"]
    }
  }
}
