  "nodemonConfig": {
    "watch": [
      "local.settings.js",
      "webpack.config.js"
    ],
    "extentions": []
  },
  "overrides": {
    "semver": "^7.6.0",
    "postcss": "^8.4.38"
  },
  "engines": {
    "node": ">= 18"
  },
  "simple-git-hooks": {
    "pre-commit": "npx lint-staged --concurrent false",
    "commit-msg": "node ./bklog/web/webpack/verify-commit.js $1"
  },
  "lint-staged": {
    "src/**/*.(vue|js|ts|tsx)": [
      "eslint --ext .js,.tsx,.ts,.vue --cache --fix"
    ],
    "src/**/*.(vue|scss|css|sass)": [
      "stylelint --cache --fix"
    ]
  }
